<#--根路径-->
<#assign basePath = "/">
<#--css根路径-->
<#assign cssPath = basePath + "css/"/>
<#--JS根路径-->
<#assign jsPath = basePath + "js/"/>
<#--图片根路径-->
<#assign imagesPath = basePath + "image/"/>
<#--插件根路径-->
<#assign plugPath = basePath + "js/plugin/"/>
<#--版权信息声明-->
<#assign copyRight = (.now?string("yyyy"))/>
<#--版本信息声明-->
<#assign version = (.now?string("yyyyMMddmmhhss"))/>